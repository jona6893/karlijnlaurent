{
  "id": "page",
  "label": "Page",
  "format": "page",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": { "type": "UID", "config": { "label": "UID", "placeholder": "" } },
      "title": {
        "type": "StructuredText",
        "config": {
          "label": "Title",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "heading1"
        }
      },
      "parent": {
        "type": "Link",
        "config": {
          "label": "Parent",
          "select": "document",
          "customtypes": ["page"]
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "hero": { "type": "SharedSlice" },
            "quote": { "type": "SharedSlice" },
            "text": { "type": "SharedSlice" },
            "image": { "type": "SharedSlice" },
            "image_cards": { "type": "SharedSlice" },
            "text_with_image": { "type": "SharedSlice" },
            "frontpage": { "type": "SharedSlice" },
            "copyright_notice": { "type": "SharedSlice" },
            "masonry_image_grid": { "type": "SharedSlice" },
            "page_title_card": { "type": "SharedSlice" }
          }
        }
      }
    }
  }
}
